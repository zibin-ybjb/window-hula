<view class="container">
  <view class="tab-bar">
    <view class="tab-bar-item" wx:for="{{list}}" wx:key="index" data-path="{{item.pagePath}}" data-index="{{index}}" bindtap="switchTab">
      <image class="img {{selected === index ? 'secItem':'unsec' }}" src="{{item.iconPath}}"></image>
    </view>
  </view>
</view>