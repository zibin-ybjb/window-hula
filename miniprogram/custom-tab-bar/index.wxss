.container{
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
}
.tab-bar {
  width: 90%;
  height: 110rpx;
  display: flex;
  justify-content: space-evenly; 
  align-items: center;
  position: fixed;
  bottom: 15px;
  border-radius: 20px;
  background: #9fb7f3;
  box-shadow: -2px -2px 3px #888888;
  overflow: hidden;
}
.tab-bar-item {
  margin: 0 60rpx;
}
.tab-bar-item .img{
  width: 60rpx;
  height: 60rpx;
  z-index: 999;
} 
.line{
  background: #000;
  margin-left: 96rpx;
  margin-bottom: 20rpx;
  height: 10rpx;
  width: 10rpx;
  border-radius: 50%;
}
